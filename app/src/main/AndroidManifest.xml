<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sidim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PictureOriginal"
            android:label="@string/title_activity_picture_original"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".YourPicture"
            android:label="@string/title_activity_your_picture"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OutPutWindow"
            android:label="@string/title_activity_out_put_window"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>